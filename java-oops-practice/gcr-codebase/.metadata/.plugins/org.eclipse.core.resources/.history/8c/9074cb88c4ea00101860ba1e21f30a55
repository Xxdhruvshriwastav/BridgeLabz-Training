package com.inheritance;

public class Vehicle {
    public int maxspeed;
    public String model;

    public Vehicle(int maxspeed, String model) {
        this.maxspeed = maxspeed;
        this.model = model;
    }

    void displayInfo() {
        System.out.println("Model: " + model);
        System.out.println("Max Speed: " + maxspeed + " km/h");
    }

}
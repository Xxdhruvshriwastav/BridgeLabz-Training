package com.inheritance;

public class VehicleTest {

    public static void main(String[] args) {
        Vehicle v1 = new Vehicle(180, "Toyota Corolla");
        ElectricVehicle ev1 = new ElectricVehicle(150, "Tesla Model 3", 75);
        PetrolVehicle pv1 = new PetrolVehicle(200, "Ford Mustang", 60);

        v1.displayInfo();
        ev1.displayInfo();
        pv1.displayInfo();
    }
}
